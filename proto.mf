mode_setup;

font_size 10pt#;
font_normal_space 4pt#;

def number(expr a,b) =
  draw (0,a)--(b,a);
  draw (0,3*a)--(2*b,3*a)--(2*b,5*a);
enddef;

def na(expr a,b) =
  draw (2*b,4*a)--(b,4*a);
enddef;

def nb(expr a,b) =
  draw (2*b,5*a)--(b,5*a);
enddef;

def nc(expr a,b) =
  draw (b,3*a)--(b,5*a);
enddef;

def nd(expr a,b) =
  draw (2*b,2*a)--(b,2*a);
enddef;

def ne(expr a,b) =
  draw (2*b,0)--(b,0);
enddef;

def nf(expr a,b) =
  draw (b,5*a)--(0,5*a);
enddef;

