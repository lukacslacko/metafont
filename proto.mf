  draw (0,0)--(0,h);
  draw (0,h)--(w,h);
  draw (w,h)--(w,0);
  draw (0,h/4)--(w,h/2);
endchar;

beginchar("Y", 10pt#, 4pt#, 0);
  draw (0,0)--(w,h);
  draw (w/2,h/2) withpen pencircle scaled 5;
endchar;

% jee
