% vu
beginchar("W", 10pt#, 10pt#, 0);
  pickup pencircle scaled 5;
  draw (0,0)--(0,h);
  draw (w,0)--(w,h);
  draw (w/5,h)--(2*w/5,0);
  draw (2*w/5, 4*h/5)--(4*w/5,4*h/5);
  draw (2*w/5, 2*h/5)--(4*w/5,2*h/5);
endchar;

% yoe
